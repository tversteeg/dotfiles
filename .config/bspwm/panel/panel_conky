background yes
out_to_console yes
out_to_x no

short_units yes
update_interval 1

TEXT
S\f8XMMS: \f5${xmms2_title} - \
${xmms2_artist} \
${xmms2_elapsed}/${xmms2_duration} \
\f8DROP: \f3${exec dropbox status | sed 's/\n/u/g'} \
\f8UP: \f2${uptime_short} \
\f8CPU: \f2${cpu cpu0}% \
\f8MEM: \f2${mem}/${memmax} \
\f8TIME: \f4${time %I:%M %P}
